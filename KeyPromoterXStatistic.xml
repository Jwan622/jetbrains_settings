<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'⌘X' or '⇧⌦'</ShortCut>
          <Description>Cut</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>39</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find...">
        <Item>
          <IdeaActionID>Find</IdeaActionID>
          <ShortCut>'⌘F' or '⌥F3'</ShortCut>
          <Description>Find...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Goto Next Splitter">
        <Item>
          <IdeaActionID>NextSplitter</IdeaActionID>
          <ShortCut>'⌥⇥'</ShortCut>
          <Description>Goto Next Splitter</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Goto Previous Splitter">
        <Item>
          <IdeaActionID>PrevSplitter</IdeaActionID>
          <ShortCut>'⌥⇧⇥'</ShortCut>
          <Description>Goto Previous Splitter</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>